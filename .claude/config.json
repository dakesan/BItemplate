{
  "mcpServers": {
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/home/oodake/BItemplate"
      ]
    },
    "context7": {
      "transport": {
        "type": "http",
        "url": "https://mcp.context7.com/mcp"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ]
    },
    "markitdown": {
      "command": "npx",
      "args": [
        "markitdown-mcp@latest"
      ]
    }
  }
}
